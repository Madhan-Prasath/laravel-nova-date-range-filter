{
    "name": "pos-lifestyle/laravel-nova-date-range-filter-nova-4",
    "description": "A Laravel Nova date range filter.",
    "keywords": [
        "laravel",
        "nova",
        "date",
        "range",
        "filter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Madhan-Prasath",
            "email": "email2madhanprasath@gmail.com",
            "homepage": "https://github.com/Madhan-Prasath",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Madhan-Prasath/laravel-nova-date-range-filter/issues"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0 || ^9.0",
        "laravel/nova": "^4.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PosLifestyle\\DateRangeFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PosLifestyle\\DateRangeFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
